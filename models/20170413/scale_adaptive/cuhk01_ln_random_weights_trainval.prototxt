name: "SRCNN_CUHK01_l_nl_Random_weights"
richness: 1000

################################## cuhk01_cam1 mixture_data #####################################
layer { name: "data" type: "Data" top: "mix_data" top: "label"
  data_param { backend: LMDB batch_size: 10 shuffle_pool_size: 10
    source: "/home/jiening/SRCNN_JSTL/external/db/cuhk01_ln_random_x2x3x4/train_cam1_lmdb" }
  transform_param { mirror: true 
    mean_value: 0 mean_value: 0 mean_value: 0 mean_value: 0 mean_value: 0 mean_value: 0
    mean_value: 0 mean_value: 0 mean_value: 0 mean_value: 102 mean_value: 102 mean_value: 101}
  include: { phase: TRAIN } }
layer { name: "data" type: "Data" top: "mix_data" top: "label"
  data_param { backend: LMDB batch_size: 5
    source: "/home/jiening/SRCNN_JSTL/external/db/cuhk01_ln_random_x2x3x4/val_cam1_lmdb" }
  transform_param { mirror: false 
    mean_value: 0 mean_value: 0 mean_value: 0 mean_value: 0 mean_value: 0 mean_value: 0
    mean_value: 0 mean_value: 0 mean_value: 0 mean_value: 102 mean_value: 102 mean_value: 101}
  include: { phase: TEST } }

##################################### cuhk01_cam0 data #####################################
layer { name: "data" type: "Data" top: "data_cam0_weights" top: "label_cam0"
  data_param { backend: LMDB batch_size: 10 shuffle_pool_size: 10
    source: "/home/jiening/SRCNN_JSTL/external/db/cuhk01_ln_random_x2x3x4/train_cam0_lmdb" }
  transform_param { mirror: true }
  include: { phase: TRAIN } }
layer { name: "data" type: "Data" top: "data_cam0_weights" top: "label_cam0"
  data_param { backend: LMDB batch_size: 5
    source: "/home/jiening/SRCNN_JSTL/external/db/cuhk01_ln_random_x2x3x4/val_cam0_lmdb" }
  transform_param { mirror: false }
  include: { phase: TEST } }

##################################### data_b and data #####################################
layer { name: "slicer1" type: "Slice" 
  bottom: "mix_data" top: "data_b_x2" top: "data_b_x3" top: "data_b_x4" top: "data"
  slice_param { axis: 1 } }

##################################### data_cam0 and weights ###############################
layer { name: "slicer1" type: "Slice" 
  bottom: "data_cam0_weights" top: "data_cam0" top: "weight_x2" top: "weight_x3" top: "weight_x4"
  slice_param { axis: 1  slice_point: 3 slice_point: 4 slice_point: 5 } }

################################# concate ##########################################
layer { 
  name: "concate_SR" 
  type: "Concat"
  bottom: "data_b_x2"
  bottom: "data_b_x3"
  bottom: "data_b_x4"
  bottom: "data_cam0"
  top: "concate_SR" 
  concat_param { axis: 0 } }

################################### bgr2ycbcr ################################
layer { name: "slicer_BGR" type: "Slice" 
  bottom: "concate_SR" 
  top: "data_B" 
  top: "data_G" 
  top: "data_R"
  slice_param { axis: 1 } }

layer { name: "eltwise_layer_Y" type: "Eltwise"
  bottom: "data_R" bottom: "data_G" bottom: "data_B" top: "eltwise_layer_Y"
  eltwise_param { operation: SUM coeff: 0.2568 coeff: 0.5041 coeff: 0.0979} }
layer{ name: "conv_Y" type: "Convolution"
  bottom: "eltwise_layer_Y" top: "conv_Y"
  param { lr_mult: 0 decay_mult: 0 } 
  param { lr_mult: 0 decay_mult: 0 }
  convolution_param { num_output: 1 kernel_size: 1 stride: 1 pad: 0
    weight_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: 16.0 } } }
layer { name: "eltwise_layer_Cb" type: "Eltwise"
  bottom: "data_R" bottom: "data_G" bottom: "data_B" top: "eltwise_layer_Cb"
  eltwise_param { operation: SUM coeff: -0.1482 coeff: -0.2910 coeff: 0.4392 } }
layer { name: "eltwise_layer_Cr" type: "Eltwise"
  bottom: "data_R" bottom: "data_G" bottom: "data_B" top: "eltwise_layer_Cr"
  eltwise_param { operation: SUM coeff: 0.4392 coeff: -0.3678 coeff: -0.0714 } }

########################## data_b with different scales ##############################
layer { name: "slicer_Y" type: "Slice" 
  bottom: "conv_Y" top: "data_b_Y_x2" top: "data_b_Y_x3" top: "data_b_Y_x4" top: "data_cam0_Y"
  slice_param { axis: 0} }

layer { name: "slicer_Cb" type: "Slice" 
  bottom: "eltwise_layer_Cb" top: "data_b_Cb_x2" top: "data_b_Cb_x3" top: "data_b_Cb_x4" top: "data_cam0_Cb"
  slice_param { axis: 0} }

layer { name: "slicer_Cr" type: "Slice" 
  bottom: "eltwise_layer_Cr" top: "data_b_Cr_x2" top: "data_b_Cr_x3" top: "data_b_Cr_x4" top: "data_cam0_Cr"
  slice_param { axis: 0} }

################################# concate_x2 ##########################################
layer { 
  name: "concate_Y_x2" 
  type: "Concat"
  bottom: "data_b_Y_x2"
  bottom: "data_cam0_Y"
  top: "concate_Y_x2" 
  concat_param { axis: 0 } }

################################## SR_Y_x2 ##################################
layer { name: "SR_conv1_x2" type: "Convolution"
  bottom: "concate_Y_x2" top: "SR_conv1_x2"
  param { lr_mult: 1.0e-8 decay_mult: 0 }
  param { lr_mult: 1.0e-9 decay_mult: 0 }
  convolution_param { num_output: 64 kernel_size: 9 stride: 1 pad: 0
    weight_filler { type: "gaussian" std: 0.001 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "SR_relu1" type: "ReLU" bottom: "SR_conv1_x2" top: "SR_conv1_x2" }

layer { name: "SR_conv2_x2" type: "Convolution"
  bottom: "SR_conv1_x2" top: "SR_conv2_x2"
  param { lr_mult: 1.0e-8 decay_mult: 0 }
  param { lr_mult: 1.0e-9 decay_mult: 0 }
  convolution_param { num_output: 32 kernel_size: 5 stride: 1 pad: 0
    weight_filler { type: "gaussian" std: 0.001 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "SR_relu2" type: "ReLU" bottom: "SR_conv2_x2" top: "SR_conv2_x2" }

################################# concate_x3 ##########################################
layer { 
  name: "concate_Y_x3" 
  type: "Concat"
  bottom: "data_b_Y_x3"
  bottom: "data_cam0_Y"
  top: "concate_Y_x3" 
  concat_param { axis: 0 } }

################################## SR_Y_x3 ##################################
layer { name: "SR_conv1_x3" type: "Convolution"
  bottom: "concate_Y_x3" top: "SR_conv1_x3"
  param { lr_mult: 1.0e-8 decay_mult: 0 }
  param { lr_mult: 1.0e-9 decay_mult: 0 }
  convolution_param { num_output: 64 kernel_size: 9 stride: 1 pad: 0
    weight_filler { type: "gaussian" std: 0.001 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "SR_relu1" type: "ReLU" bottom: "SR_conv1_x3" top: "SR_conv1_x3" }

layer { name: "SR_conv2_x3" type: "Convolution"
  bottom: "SR_conv1_x3" top: "SR_conv2_x3"
  param { lr_mult: 1.0e-8 decay_mult: 0 }
  param { lr_mult: 1.0e-9 decay_mult: 0 }
  convolution_param { num_output: 32 kernel_size: 5 stride: 1 pad: 0
    weight_filler { type: "gaussian" std: 0.001 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "SR_relu2" type: "ReLU" bottom: "SR_conv2_x3" top: "SR_conv2_x3" }

################################# concate_x4 ##########################################
layer { 
  name: "concate_Y_x4" 
  type: "Concat"
  bottom: "data_b_Y_x4"
  bottom: "data_cam0_Y"
  top: "concate_Y_x4" 
  concat_param { axis: 0 } }

################################## SR_Y_x4 ##################################
layer { name: "SR_conv1_x4" type: "Convolution"
  bottom: "concate_Y_x4" top: "SR_conv1_x4"
  param { lr_mult: 1.0e-8 decay_mult: 0 }
  param { lr_mult: 1.0e-9 decay_mult: 0 }
  convolution_param { num_output: 64 kernel_size: 9 stride: 1 pad: 0
    weight_filler { type: "gaussian" std: 0.001 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "SR_relu1" type: "ReLU" bottom: "SR_conv1_x4" top: "SR_conv1_x4" }

layer { name: "SR_conv2_x4" type: "Convolution"
  bottom: "SR_conv1_x4" top: "SR_conv2_x4"
  param { lr_mult: 1.0e-8 decay_mult: 0 }
  param { lr_mult: 1.0e-9 decay_mult: 0 }
  convolution_param { num_output: 32 kernel_size: 5 stride: 1 pad: 0
    weight_filler { type: "gaussian" std: 0.001 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "SR_relu2" type: "ReLU" bottom: "SR_conv2_x4" top: "SR_conv2_x4" }

################################# concate_x2x3x4 ##########################################
layer { 
  name: "concate_SR_conv2" 
  type: "Concat"
  bottom: "SR_conv2_x2"
  bottom: "SR_conv2_x3"
  bottom: "SR_conv2_x4"
  top: "concate_SR_conv2" 
  concat_param { axis: 0 } }

################################ Shared SR_conv3 ##################################
layer { name: "SR_conv3" type: "Convolution" 
  bottom: "concate_SR_conv2" top: "SR_conv3"
  param { lr_mult: 1.0e-9 decay_mult: 0 }
  param { lr_mult: 1.0e-9 decay_mult: 0 }
  convolution_param { num_output: 1 kernel_size: 5 stride: 1 pad: 0
    weight_filler { type: "gaussian" std: 0.001 }
    bias_filler { type: "constant" value: 0 } } }

################################# concate ##########################################
layer { 
  name: "concate_Cb" 
  type: "Concat"
  bottom: "data_b_Cb_x2"
  bottom: "data_cam0_Cb"
  bottom: "data_b_Cb_x3"
  bottom: "data_cam0_Cb"
  bottom: "data_b_Cb_x4"
  bottom: "data_cam0_Cb"
  top: "concate_Cb" 
  concat_param { axis: 0 } }

layer { 
  name: "concate_Cr" 
  type: "Concat"
  bottom: "data_b_Cr_x2"
  bottom: "data_cam0_Cr"
  bottom: "data_b_Cr_x3"
  bottom: "data_cam0_Cr"
  bottom: "data_b_Cr_x4"
  bottom: "data_cam0_Cr"
  top: "concate_Cr" 
  concat_param { axis: 0 } }

################################# ycbcr2bgr ################################
layer { name: "crop_Cb" type: "Crop"
  bottom: "concate_Cb" bottom: "SR_conv3" top: "crop_Cb"
  crop_param { axis: 2 offset: 8 } }

layer { name: "crop_Cr" type: "Crop"
  bottom: "concate_Cr" bottom: "SR_conv3" top: "crop_Cr"
  crop_param { axis: 2 offset: 8 } }

layer{ name: "conv_Y_2" type: "Convolution"
  bottom: "SR_conv3" top: "conv_Y_2"
  param { lr_mult: 0 decay_mult: 0 }
  param { lr_mult: 0 decay_mult: 0 }
  convolution_param { num_output: 1 kernel_size: 1 stride: 1 pad: 0
    weight_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: -16.0 } } }
layer { name: "eltwise_layer_R" type: "Eltwise"
  bottom: "conv_Y_2" bottom: "crop_Cb" bottom: "crop_Cr" top: "eltwise_layer_R"
  eltwise_param { operation: SUM coeff: 1.1644 coeff: 0.0000 coeff: 1.5960 } }

layer { name: "eltwise_layer_R_relu1" type: "ReLU" bottom: "eltwise_layer_R" top: "eltwise_layer_R" }
layer { name: "eltwise_layer_R_conv1" type: "Convolution"
  bottom: "eltwise_layer_R" top: "eltwise_layer_R_conv1"
  param { lr_mult: 0 decay_mult: 0 }
  param { lr_mult: 0 decay_mult: 0 }
  convolution_param { num_output: 1 kernel_size: 1 stride: 1 pad: 0
    weight_filler { type: "constant" value: -1}
    bias_filler { type: "constant" value: 255 } } }
layer { name: "eltwise_layer_R_relu2" type: "ReLU" bottom: "eltwise_layer_R_conv1" top: "eltwise_layer_R_conv1" }
layer { name: "eltwise_layer_R_conv2" type: "Convolution"
  bottom: "eltwise_layer_R_conv1" top: "eltwise_layer_R_conv2"
  param { lr_mult: 0 decay_mult: 0 }
  param { lr_mult: 0 decay_mult: 0 }
  convolution_param { num_output: 1 kernel_size: 1 stride: 1 pad: 0
    weight_filler { type: "constant" value: -1}
    bias_filler { type: "constant" value: 255 } } }

layer { name: "eltwise_layer_G" type: "Eltwise"
  bottom: "conv_Y_2" bottom: "crop_Cb" bottom: "crop_Cr" top: "eltwise_layer_G"
  eltwise_param { operation: SUM coeff: 1.1644 coeff: -0.3918 coeff: -0.8130 } }

layer { name: "eltwise_layer_G_relu1" type: "ReLU" bottom: "eltwise_layer_G" top: "eltwise_layer_G" }
layer { name: "eltwise_layer_G_conv1" type: "Convolution"
  bottom: "eltwise_layer_G" top: "eltwise_layer_G_conv1"
  param { lr_mult: 0 decay_mult: 0 }
  param { lr_mult: 0 decay_mult: 0 }
  convolution_param { num_output: 1 kernel_size: 1 stride: 1 pad: 0
    weight_filler { type: "constant" value: -1}
    bias_filler { type: "constant" value: 255 } } }
layer { name: "eltwise_layer_G_relu2" type: "ReLU" bottom: "eltwise_layer_G_conv1" top: "eltwise_layer_G_conv1" }
layer { name: "eltwise_layer_G_conv2" type: "Convolution"
  bottom: "eltwise_layer_G_conv1" top: "eltwise_layer_G_conv2"
  param { lr_mult: 0 decay_mult: 0 }
  param { lr_mult: 0 decay_mult: 0 }
  convolution_param { num_output: 1 kernel_size: 1 stride: 1 pad: 0
    weight_filler { type: "constant" value: -1}
    bias_filler { type: "constant" value: 255 } } }

layer { name: "eltwise_layer_B" type: "Eltwise"
  bottom: "conv_Y_2" bottom: "crop_Cb" bottom: "crop_Cr" top: "eltwise_layer_B"
  eltwise_param { operation: SUM coeff: 1.1644 coeff: 2.0172 coeff: 0.0000 } }

layer { name: "eltwise_layer_B_relu1" type: "ReLU" bottom: "eltwise_layer_B" top: "eltwise_layer_B" }
layer { name: "eltwise_layer_B_conv1" type: "Convolution"
  bottom: "eltwise_layer_B" top: "eltwise_layer_B_conv1"
  param { lr_mult: 0 decay_mult: 0 }
  param { lr_mult: 0 decay_mult: 0 }
  convolution_param { num_output: 1 kernel_size: 1 stride: 1 pad: 0
    weight_filler { type: "constant" value: -1}
    bias_filler { type: "constant" value: 255 } } }
layer { name: "eltwise_layer_B_relu2" type: "ReLU" bottom: "eltwise_layer_B_conv1" top: "eltwise_layer_B_conv1" }
layer { name: "eltwise_layer_B_conv2" type: "Convolution"
  bottom: "eltwise_layer_B_conv1" top: "eltwise_layer_B_conv2"
  param { lr_mult: 0 decay_mult: 0 }
  param { lr_mult: 0 decay_mult: 0 }
  convolution_param { num_output: 1 kernel_size: 1 stride: 1 pad: 0
    weight_filler { type: "constant" value: -1}
    bias_filler { type: "constant" value: 255 } } }

#################################### weights ##################################
layer { name: "slicer_B" type: "Slice" 
  bottom: "eltwise_layer_B_conv2" 
  top: "data_cam1_B_x2" top: "data_cam0_B_x2"
  top: "data_cam1_B_x3" top: "data_cam0_B_x3" 
  top: "data_cam1_B_x4" top: "data_cam0_B_x4"
  slice_param { axis: 0 } }

layer { name: "slicer_G" type: "Slice" 
  bottom: "eltwise_layer_G_conv2" 
  top: "data_cam1_G_x2" top: "data_cam0_G_x2"
  top: "data_cam1_G_x3" top: "data_cam0_G_x3" 
  top: "data_cam1_G_x4" top: "data_cam0_G_x4"
  slice_param { axis: 0 } }

layer { name: "slicer_R" type: "Slice" 
  bottom: "eltwise_layer_R_conv2" 
  top: "data_cam1_R_x2" top: "data_cam0_R_x2"
  top: "data_cam1_R_x3" top: "data_cam0_R_x3" 
  top: "data_cam1_R_x4" top: "data_cam0_R_x4"
  slice_param { axis: 0 } }

layer { name: "crop_Cb" type: "Crop"
  bottom: "weight_x2" bottom: "data_cam0_B_x2" top: "crop_weight_x2"
  crop_param { axis: 2 } }
layer { name: "crop_Cb" type: "Crop"
  bottom: "weight_x3" bottom: "data_cam0_B_x3" top: "crop_weight_x3"
  crop_param { axis: 2 } }
layer { name: "crop_Cb" type: "Crop"
  bottom: "weight_x4" bottom: "data_cam0_B_x4" top: "crop_weight_x4"
  crop_param { axis: 2 } }

layer{ name: "eltwise_B_x2" type: "Eltwise"
  bottom: "data_cam0_B_x2" bottom: "crop_weight_x2" top: "data_cam0_B_x2_weight"
  eltwise_param { operation: PROD } }
layer{ name: "eltwise_B_x3" type: "Eltwise"
  bottom: "data_cam0_B_x3" bottom: "crop_weight_x3" top: "data_cam0_B_x3_weight"
  eltwise_param { operation: PROD } }
layer{ name: "eltwise_B_x4" type: "Eltwise"
  bottom: "data_cam0_B_x4" bottom: "crop_weight_x4" top: "data_cam0_B_x4_weight"
  eltwise_param { operation: PROD } }  

layer{ name: "eltwise_G_x2" type: "Eltwise"
  bottom: "data_cam0_G_x2" bottom: "crop_weight_x2" top: "data_cam0_G_x2_weight"
  eltwise_param { operation: PROD } }
layer{ name: "eltwise_G_x3" type: "Eltwise"
  bottom: "data_cam0_G_x3" bottom: "crop_weight_x3" top: "data_cam0_G_x3_weight"
  eltwise_param { operation: PROD } }
layer{ name: "eltwise_G_x4" type: "Eltwise"
  bottom: "data_cam0_G_x4" bottom: "crop_weight_x4" top: "data_cam0_G_x4_weight"
  eltwise_param { operation: PROD } }

layer{ name: "eltwise_R_x2" type: "Eltwise"
  bottom: "data_cam0_R_x2" bottom: "crop_weight_x2" top: "data_cam0_R_x2_weight"
  eltwise_param { operation: PROD } }
layer{ name: "eltwise_R_x3" type: "Eltwise"
  bottom: "data_cam0_R_x3" bottom: "crop_weight_x3" top: "data_cam0_R_x3_weight"
  eltwise_param { operation: PROD } }
layer{ name: "eltwise_R_x4" type: "Eltwise"
  bottom: "data_cam0_R_x4" bottom: "crop_weight_x4" top: "data_cam0_R_x4_weight"
  eltwise_param { operation: PROD } }

layer { name: "eltwise_B" type: "Eltwise"
  bottom: "data_cam0_B_x2_weight" bottom: "data_cam0_B_x3_weight" bottom: "data_cam0_B_x4_weight" top: "eltwise_B"
  eltwise_param { operation: SUM } }
layer { name: "eltwise_G" type: "Eltwise"
  bottom: "data_cam0_G_x2_weight" bottom: "data_cam0_G_x3_weight" bottom: "data_cam0_G_x4_weight" top: "eltwise_G"
  eltwise_param { operation: SUM } }
layer { name: "eltwise_R" type: "Eltwise"
  bottom: "data_cam0_R_x2_weight" bottom: "data_cam0_R_x3_weight" bottom: "data_cam0_R_x4_weight" top: "eltwise_R"
  eltwise_param { operation: SUM } }

layer { 
  name: "concate_B" 
  type: "Concat"
  bottom: "data_cam1_B_x2"
  bottom: "data_cam1_B_x3"
  bottom: "data_cam1_B_x4"
  bottom: "eltwise_B"
  top: "concate_B" 
  concat_param { axis: 0 } }

layer { 
  name: "concate_G" 
  type: "Concat"
  bottom: "data_cam1_G_x2"
  bottom: "data_cam1_G_x3"
  bottom: "data_cam1_G_x4"
  bottom: "eltwise_G"
  top: "concate_G" 
  concat_param { axis: 0 } }

layer { 
  name: "concate_R" 
  type: "Concat"
  bottom: "data_cam1_R_x2"
  bottom: "data_cam1_R_x3"
  bottom: "data_cam1_R_x4"
  bottom: "eltwise_R"
  top: "concate_R" 
  concat_param { axis: 0 } }

################################## reduce mean ##################################
layer{ name: "conv_B" type: "Convolution"
  bottom: "concate_B" top: "conv_B"
  param { lr_mult: 0 decay_mult: 0 }
  param { lr_mult: 0 decay_mult: 0 }
  convolution_param { num_output: 1 kernel_size: 1 stride: 1 pad: 0
    weight_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: -102 } } }
layer{ name: "conv_G" type: "Convolution"
  bottom: "concate_G" top: "conv_G"
  param { lr_mult: 0 decay_mult: 0 }
  param { lr_mult: 0 decay_mult: 0 }
  convolution_param { num_output: 1 kernel_size: 1 stride: 1 pad: 0
    weight_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: -102 } } }
layer{ name: "conv_R" type: "Convolution"
  bottom: "concate_R" top: "conv_R"
  param { lr_mult: 0 decay_mult: 0 }
  param { lr_mult: 0 decay_mult: 0 }
  convolution_param { num_output: 1 kernel_size: 1 stride: 1 pad: 0
    weight_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: -101 } } }
layer {  name: "data_RGB"  
  type: "Concat"
  bottom: "conv_B"
  bottom: "conv_G"
  bottom: "conv_R"
  top: "data_RGB"
}

layer { name: "slicer_cam" type: "Slice" 
  bottom: "data_RGB" 
  top: "data_cam1_RGB_x2" 
  top: "data_cam1_RGB_x3" 
  top: "data_cam1_RGB_x4" 
  top: "data_cam0_RGB"
  slice_param { axis: 0 } }

################################## SR loss ##################################
layer { name: "crop_data" type: "Crop"
  bottom: "data" bottom: "data_cam1_RGB_x2" top: "crop_data"
  crop_param { axis: 2 offset: 8 } }
